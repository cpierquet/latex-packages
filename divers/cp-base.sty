\newcommand\convdecbin[1]{%
	\def\resbrut{\xintDecToBin{#1}}
	\StrLen{\resbrut}[\nbchiffres]
	\def\nbgrp{\fpeval{4*ceil(\nbchiffres/4,0)}}
	\num[digit-group-size=4,minimum-integer-digits=\nbgrp]{\resbrut}
}

